{
	"manifest_version": 2,

	"name": "FreshIT — Getting Started",
	"description": "Sample extension for the “Making your first extension” article",
	"version": "1.0",

	"background": {
		"scripts": ["jquery-3.2.1.js", "background.js"],
        "persistent": false
    },

    "browser_action": {
        "default_popup": "popup.html"
    },

    "content_scripts": [ {
        "js": [ "jquery-3.2.1.js", "contentscript.js" ],
        "matches": ["*://base.freshit.ua/*"],
        "run_at": "document_end"	
    }],

	"permissions": ["activeTab", "contextMenus", "tabs"]
}
